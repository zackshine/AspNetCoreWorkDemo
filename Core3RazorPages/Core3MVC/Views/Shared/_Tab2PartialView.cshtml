

@model List<Core3MVC.Controllers.NameModel>

@{
    ViewData["Title"] = "Index";
}

<h1>_Tab2PartialView</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model[0].Name)
            </th>
            
            <th></th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count(); i++)
        {
            <tr>
                <td>
                    @Model[i].Name
                </td>
                 <td></td>
            </tr>
        }
    </tbody>
</table>